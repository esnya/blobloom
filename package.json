{
  "name": "blobloom",
  "version": "0.0.1",
  "private": true,
  "description": "Git size-history visualizer",
  "type": "module",
  "scripts": {
    "start": "npm run build && tsx src/server.ts",
    "lint": "eslint \"**/*.ts?(x)\" --max-warnings=0",
    "build": "vite build",
    "dev": "vite",
    "test": "jest",
    "test:playwright": "playwright test",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "rename-demo": "tsx src/renameDemo.ts"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "express": "^5.1.0",
    "isomorphic-git": "^1.30.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@eslint/eslintrc": "^3.3.1",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "playwright": "^1.53.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.6.3",
    "typescript": "^5.2.2",
    "vite": "^6.3.5",
    "@vitejs/plugin-react": "^4.5.2"
  },
  "overrides": {
    "test-exclude": "^7.0.1"
  },
  "engines": {
    "node": ">=20"
  }
}
